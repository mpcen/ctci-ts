{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier", "gts/tslint.json"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "interface-name": false,
    "no-console": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "jsx-self-close": false,
    "no-debugger": false,
    "object-literal-sort-keys": false
  }
}